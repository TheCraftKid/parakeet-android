<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Parakeet" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="Parakeet.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Parakeet.AppBarOverlay" parent="ThemeOverlay.AppCompat.ActionBar" />

    <style name="Parakeet.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>